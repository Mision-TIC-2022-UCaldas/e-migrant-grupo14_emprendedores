@page
@model E_Migrant.App.Frontend.Pages.ListadoEntidades.ListadoEntidadesModel
@{
}

<form method="get" style="float: right">
            <div class="form-group">
                Nit: <input type="text" name="Filtro" asp-for="Filtro" />
                <input type="submit" value="Filtrar" />
            </div>
</form>

<table class="table">
    <thead>
        <tr>
            <th >id</th>
            <th>Razon Social</th>
            <th>NIT</th>
            <th>Dirección</th>
            <th>Telefono</th>
            <th>email</th>
            <th>Pagina web</th>
            <th>ciudad</th>
            <th>Sector</th>
            <th>Tipo Servicio</th>
        </tr>
    </thead>
   @foreach (var entidad in Model.datosEntidadesColaboradoras)
    {
        <tr>
            <td >@entidad.id</td>
            <td>@entidad.razonSocial</td>
            <td>@entidad.nit</td>
            <td>@entidad.direccion</td>
            <td>@entidad.Telefono</td>
            <td>@entidad.correoElectronico</td>
            <td>@entidad.paginaWeb</td>
            <td>@entidad.ciudad</td>
            <td>@entidad.sector</td>
            <td>@entidad.serviciosOfrece</td>
        </tr>
    }

</table>